<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="50dp"
    tools:ignore="MissingPrefix"
    android:orientation="vertical"
    tools:context="com.example.aalam.dashboardapp.EligibleActivity">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Ineligible"
        android:paddingLeft="50dp"
        fontPath="fonts/HelveticaNeueThin.ttf"
        android:gravity="center"
        android:textSize="30dp"
        android:padding="30dp"
        android:textColor="@color/materialPink"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Unfortunately, your answers to the questions indicate you are ineligible at this time"
        fontPath="fonts/HelveticaNeueThin.ttf"
        android:gravity="center"
        android:textSize="20dp"
        android:paddingTop="10dp"
        android:textColor="@color/materialPink"/>


</LinearLayout>
